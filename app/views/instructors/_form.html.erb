<%= form_for(@instructor) do |f| %>
    <div class="form"><%= f.label :name %></div>
    <div class="form"><%= f.text_field :name %></div>
    <div class="form"><%= f.label :bio %></div>
    <div class="form"><%= f.text_area :bio %></div>
    <div class="form"><%= f.label :email %></div>
    <div class="form"><%= f.email_field :email %></div>
    <div class="form"><%= f.label :password %></div>
    <div class="form"><%= f.password_field :password %></div>
    <div class="form"><%= f.label :password_confirmation %></div>
    <div class="form"><%= f.password_field :password %></div>
  
    <%= f.submit "Submit"%>
<% end %>